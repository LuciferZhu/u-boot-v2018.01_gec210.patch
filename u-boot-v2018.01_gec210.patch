+void Write_LDI_TL2796(u16 uAddr, u16 uData)
+{
+	s32 j;
+
+	if(uAddr != 0x0000)
+	{
+		LCDM_CS_HIGH;
+		LCDM_SCL_HIGH;
+		LCDM_SDA_HIGH;
+
+		LCDM_CS_LOW;
+		LCDM_SCL_LOW;
+
+	    // Command
+	    for (j = 15 ; j >= 0; j--)
+		{
+			LCDM_SCL_LOW;
+
+			if ((uAddr >> j) & 0x0001)
+			{
+				LCDM_SDA_HIGH;
+			}
+			else
+			{
+				LCDM_SDA_LOW;
+			}
+			LCDM_SCL_HIGH;
+		}
+	}
+
+	if (uAddr == 0x0000)
+	{
+		LCDM_SDA_HIGH;
+	}
+
+	LCDM_CS_HIGH;
+	LCDM_SCL_HIGH;
+	LCDM_CS_LOW;
+	LCDM_SCL_LOW;
+
+	// parameter
+	for (j = 15 ; j >= 0; j--)
+	{
+		LCDM_SCL_LOW;
+
+		if ((uData >> j) & 0x0001)
+		{
+			LCDM_SDA_HIGH;
+		}
+		else
+		{
+			LCDM_SDA_LOW;
+		}
+		LCDM_SCL_HIGH;
+	}
+}
